<h1>The Gatopan Family (circa 103 b.c.)</h1>
<div>
    <table>
      <thead>
        <% Person.column_names.each do |header| %>
          <th><%= header %></th>
        <% end %>
      </thead>
      <tbody>
        <% @people.each do |person| %>
          <tr>
            <% person.attributes.each do |k,v| %>
            <td><%= v %></td>
            <% end %>
          </tr>
        <% end %>
      </tbody>
    </table>
</div>
